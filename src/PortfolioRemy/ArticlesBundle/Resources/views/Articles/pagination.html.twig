{# affichage boutons pagination #}
<div class="pagination">
    <ul >
    {% for p in range(1, nombrePage) %}
            <li{% if p == page %} class="active" {% endif %}>
                <a href="{{ path('portfolioremyarticles_accueil', {'page': p}) }}">{{ p }}</a>
            </li>
     {% endfor %}
    </ul>
</div>